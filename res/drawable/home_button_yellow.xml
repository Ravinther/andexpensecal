<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#f3ae1b" />
            <stroke android:width="1dp" android:color="#bb6008" />
            <corners android:radius="3dp" />
            <padding android:left="2dp" android:top="4dp" android:right="2dp" android:bottom="4dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#f5ae55" android:endColor="#e77902" android:angle="90.0" />
            <stroke android:width="1dp" android:color="#f3ae1b" />
            <corners android:radius="4dp" />
            <padding android:left="2dp" android:top="4dp" android:right="2dp" android:bottom="4dp" />
        </shape>
    </item>
</selector>
