<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#75890c" />
            <stroke android:width="1dp" android:color="#75890c" />
            <corners android:radius="3dp" />
            <padding android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#a4b357" android:endColor="#75890c" android:angle="90.0" />
            <stroke android:width="1dp" android:color="#75890c" />
            <corners android:radius="4dp" />
            <padding android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp" />
        </shape>
    </item>
</selector>
